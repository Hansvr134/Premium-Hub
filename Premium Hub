      FileName = "Hanz Hub"
   },
   Discord = {
      Enabled = true,
      Invite = "ECxQFc97F5",
      RememberJoins = true
   },
   KeySystem = true,
   KeySettings = {
      Title = "Han Key",
      Subtitle = "Key system",
      Note = "Mau Key? Buy Key Premium",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"FFMN_jfgue786eh767et75egyf5"}
   }
})

local Tab = Window:CreateTab("Main Menu", 4483362458)
local Section = Tab:CreateSection("Menu")

local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local noclipConnection = nil

local Toggle = Tab:CreateToggle({
   Name = "Noclip",
   CurrentValue = false,
   Flag = "ToggleNoclip",
   Callback = function(Value)
      if Value then
         -- Nyalakan Noclip
         noclipConnection = RunService.Stepped:Connect(function()
            local character = LocalPlayer.Character
            if character then
               for _, part in pairs(character:GetDescendants()) do
                  if part:IsA("BasePart") then
                     part.CanCollide = false
                  end
               end
            end
         end)
      else
         -- Matikan Noclip
         if noclipConnection then
            noclipConnection:Disconnect()
            noclipConnection = nil
         end

         -- Kembalikan CanCollide true (opsional)
         local character = LocalPlayer.Character
         if character then
            for _, part in pairs(character:GetDescendants()) do
               if part:IsA("BasePart") then
                  part.CanCollide = true
               end
            end
         end
      end
   end,
})

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local walkSpeedEnabled = false
local currentSpeed = 16 -- default awal

-- üîò Toggle ON/OFF
local Toggle = Tab:CreateToggle({
    Name = "WalkSpeed Aktif",
    CurrentValue = false,
    Flag = "ToggleWalkSpeed",
    Callback = function(enabled)
        walkSpeedEnabled = enabled

        local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local humanoid = char:FindFirstChildOfClass("Humanoid")
        if humanoid then
            if walkSpeedEnabled then
                humanoid.WalkSpeed = currentSpeed
            else
                humanoid.WalkSpeed = 16 -- default
            end
        end
    end,
})

-- üìè Slider
local Slider = Tab:CreateSlider({
    Name = "WalkSpeed",
    Range = {16, 300},
    Increment = 10,
    Suffix = "Speed",
    CurrentValue = 16,
    Flag = "SliderWalkSpeed",
    Callback = function(value)
        currentSpeed = value

        if walkSpeedEnabled then
            local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
            local humanoid = char:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = currentSpeed
            end
        end
    end,
})

local Slider = Tab:CreateSlider({
    Name = "JumpPower",
    Range = {50, 300},         
    Increment = 10,
    Suffix = "JumpPower",
    CurrentValue = 50,
    Flag = "Slider1",  
    Callback = function(v)
        local player = game.Players.LocalPlayer
        if player and player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.JumpPower = v
        end
    end,
})


