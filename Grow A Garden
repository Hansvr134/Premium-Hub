local Tab = Window:CreateTab("üè† Main", nil)
local Section = Tab:CreateSection("Main Hub")

local fruitFolder = workspace.Farm.Farm.Important.Plants_Physical
local player = game.Players.LocalPlayer
local humanoidRootPart = player.Character:WaitForChild("HumanoidRootPart")
local collectDistance = 10

local function getPosition(object)
    if object:IsA("BasePart") then
        return object.Position
    elseif object:IsA("Model") then
        if object.PrimaryPart then
            return object.PrimaryPart.Position
        else
            local part = object:FindFirstChildWhichIsA("BasePart")
            if part then
                return part.Position
            end
        end
    end
    return nil
end

local function autoCollect()
    for _, fruit in pairs(fruitFolder:GetChildren()) do
        local fruitPosition = getPosition(fruit)
        if fruitPosition then
            local distance = (humanoidRootPart.Position - fruitPosition).Magnitude
            if distance <= collectDistance then
                print("Collecting:", fruit.Name)
                fruit:Destroy()
            end
        else
            warn(fruit.Name .. " tidak punya posisi!")
        end
    end
end

while task.wait(1) do
    autoCollect()
end

local Tab = Window:CreateTab("üó∫Ô∏èTeleport", nil)
